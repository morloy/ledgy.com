{
  "name": "ledgy-landing-page",
  "description": "Ledgy Landing Page",
  "version": "1.0.0",
  "author": "Ledgy AG <contact@ledgy.com>",
  "repository": "morloy/ledgy-app",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "compile": "lingui compile",
    "build": "lingui compile && gatsby build",
    "serve": "npm run build && gatsby serve",
    "start": "lingui compile && gatsby develop",
    "test": "npm run extract && lingui compile --strict && eslint . --ext jsx,js && flow",
    "extract": "lingui extract --clean",
    "edit": "micro -l tcp://0.0.0.0:$(((RANDOM % 65535)+1025)) flask_editor.js"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "7.3.4",
    "@fortawesome/fontawesome-svg-core": "1.2.15",
    "@fortawesome/free-brands-svg-icons": "5.7.2",
    "@fortawesome/free-solid-svg-icons": "5.7.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@lingui/react": "2.7.4",
    "@mdx-js/mdx": "0.20.3",
    "@mdx-js/tag": "0.20.3",
    "aos": "2.3.4",
    "babel-core": "7.0.0-bridge.0",
    "bootstrap": "4.3.1",
    "gatsby": "2.1.27",
    "gatsby-image": "2.0.31",
    "gatsby-mdx": "0.4.2",
    "gatsby-plugin-catch-links": "2.0.12",
    "gatsby-plugin-flow": "1.0.2",
    "gatsby-plugin-google-tagmanager": "2.0.9",
    "gatsby-plugin-layout": "1.0.12",
    "gatsby-plugin-manifest": "2.0.22",
    "gatsby-plugin-netlify": "2.0.11",
    "gatsby-plugin-netlify-cache": "1.1.0",
    "gatsby-plugin-nprogress": "2.0.9",
    "gatsby-plugin-offline": "2.0.24",
    "gatsby-plugin-react-helmet": "3.0.8",
    "gatsby-plugin-sass": "2.0.10",
    "gatsby-plugin-sharp": "2.0.25",
    "gatsby-plugin-sitemap": "2.0.6",
    "gatsby-plugin-sri": "1.0.6",
    "gatsby-remark-copy-linked-files": "2.0.9",
    "gatsby-remark-katex": "3.0.3",
    "gatsby-remark-prismjs": "3.2.4",
    "gatsby-source-filesystem": "2.0.23",
    "gatsby-transformer-sharp": "2.1.15",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.3.1",
    "katex": "0.10.1",
    "lodash": "4.17.11",
    "micro": "9.3.3",
    "node-sass": "4.11.0",
    "popper.js": "1.14.7",
    "prism-themes": "1.0.1",
    "prismjs": "1.15.0",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-helmet": "5.2.0",
    "typeface-slabo-27px": "0.0.72",
    "typeface-work-sans": "0.0.72"
  },
  "keywords": [
    "ledgy"
  ],
  "devDependencies": {
    "@lingui/babel-preset-react": "2.7.4",
    "@lingui/cli": "2.7.4",
    "babel-eslint": "9.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "flow-bin": "0.94.0",
    "prettier": "1.16.4"
  },
  "lingui": {
    "sourceLocale": "en",
    "localeDir": "src/locale",
    "srcPathDirs": [
      "src/pages",
      "src/components",
      "src/layouts"
    ],
    "format": "po",
    "extractBabelOptions": {
      "presets": [
        "@babel/react",
        "@babel/flow"
      ]
    }
  }
}
