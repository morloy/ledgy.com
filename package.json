{
  "name": "ledgy-landing-page",
  "description": "Ledgy Landing Page",
  "version": "1.0.0",
  "author": "Ledgy AG <contact@ledgy.com>",
  "repository": "Ledgy/ledgy.com",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "compile": "lingui compile",
    "build": "./build.sh",
    "serve": "npm run build && gatsby serve",
    "start": "./start.sh",
    "test": "eslint . --ext jsx,js && flow",
    "clean": "gatsby clean",
    "extract": "lingui extract --clean",
    "refresh": "curl -X POST http://localhost:8000/__refresh",
    "edit": "micro -l tcp://0.0.0.0:$(((RANDOM % 65535)+1025)) flask_editor.js"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/free-brands-svg-icons": "5.15.2",
    "@fortawesome/free-solid-svg-icons": "5.15.2",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@ledgy/pricing": "2.0.3",
    "@lingui/react": "2.9.2",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "aos": "2.3.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-remove-graphql-queries": "2.15.0",
    "bootstrap": "4.6.0",
    "gatsby": "2.31.1",
    "gatsby-image": "2.10.0",
    "gatsby-plugin-catch-links": "2.9.0",
    "gatsby-plugin-flow": "1.9.0",
    "gatsby-plugin-layout": "1.9.0",
    "gatsby-plugin-manifest": "2.11.0",
    "gatsby-plugin-mdx": "1.9.0",
    "gatsby-plugin-netlify": "2.10.0",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-nprogress": "2.9.0",
    "gatsby-plugin-offline": "3.9.0",
    "gatsby-plugin-react-helmet": "3.9.0",
    "gatsby-plugin-sass": "3.1.0",
    "gatsby-plugin-sharp": "2.13.3",
    "gatsby-plugin-sitemap": "2.11.0",
    "gatsby-remark-copy-linked-files": "2.9.0",
    "gatsby-remark-katex": "3.9.0",
    "gatsby-remark-prismjs": "3.12.0",
    "gatsby-remark-unwrap-images": "1.0.2",
    "gatsby-source-contentful": "4.5.2",
    "gatsby-source-filesystem": "2.10.0",
    "gatsby-transformer-sharp": "2.11.0",
    "isomorphic-fetch": "3.0.0",
    "katex": "0.12.0",
    "lity": "2.4.1",
    "lodash": "4.17.20",
    "micro": "9.3.4",
    "node-sass": "4.14.1",
    "popper.js": "1.16.1",
    "prism-themes": "1.5.0",
    "prismjs": "1.23.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-transition-group": "4.4.1",
    "typeface-open-sans": "0.0.75"
  },
  "keywords": [
    "ledgy"
  ],
  "devDependencies": {
    "@lingui/babel-preset-react": "2.9.2",
    "@lingui/cli": "2.9.2",
    "babel-eslint": "9.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.20.6",
    "flow-bin": "0.132.0",
    "npm-check-updates": "11.1.1",
    "prettier": "2.2.1"
  },
  "lingui": {
    "sourceLocale": "en",
    "localeDir": "src/locale",
    "srcPathDirs": [
      "src/pages",
      "src/components",
      "src/layouts",
      "src/helpers"
    ],
    "format": "po",
    "extractBabelOptions": {
      "presets": [
        "@babel/react",
        "@babel/flow"
      ]
    }
  }
}
