{
  "name": "ledgy-landing-page",
  "description": "Ledgy Landing Page",
  "version": "1.0.0",
  "author": "Ledgy AG <contact@ledgy.com>",
  "repository": "morloy/ledgy-app",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "compile": "lingui compile",
    "build": "./build.sh",
    "serve": "npm run build && gatsby serve",
    "start": "./start.sh",
    "test": "eslint . --ext jsx,js && flow",
    "extract": "lingui extract --clean",
    "refresh": "curl -X POST http://localhost:8000/__refresh",
    "edit": "micro -l tcp://0.0.0.0:$(((RANDOM % 65535)+1025)) flask_editor.js"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@fortawesome/fontawesome-svg-core": "1.2.21",
    "@fortawesome/free-brands-svg-icons": "5.10.1",
    "@fortawesome/free-solid-svg-icons": "5.10.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@lingui/react": "2.8.3",
    "@mdx-js/mdx": "1.3.0",
    "@mdx-js/react": "1.3.0",
    "aos": "2.3.4",
    "babel-core": "7.0.0-bridge.0",
    "bootstrap": "4.3.1",
    "gatsby": "2.13.67",
    "gatsby-image": "2.2.8",
    "gatsby-plugin-catch-links": "2.1.3",
    "gatsby-plugin-flow": "1.1.2",
    "gatsby-plugin-google-analytics": "2.1.7",
    "gatsby-plugin-layout": "1.1.3",
    "gatsby-plugin-manifest": "2.2.5",
    "gatsby-plugin-mdx": "1.0.24",
    "gatsby-plugin-netlify": "2.1.5",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-nprogress": "2.1.2",
    "gatsby-plugin-offline": "2.2.6",
    "gatsby-plugin-react-helmet": "3.1.3",
    "gatsby-plugin-sass": "2.1.8",
    "gatsby-plugin-sharp": "2.2.12",
    "gatsby-plugin-sitemap": "2.2.6",
    "gatsby-remark-copy-linked-files": "2.1.6",
    "gatsby-remark-katex": "3.1.2",
    "gatsby-remark-prismjs": "3.3.5",
    "gatsby-remark-unwrap-images": "1.0.1",
    "gatsby-source-contentful": "2.1.20",
    "gatsby-source-filesystem": "2.1.9",
    "gatsby-transformer-sharp": "2.2.6",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.4.1",
    "katex": "0.11.0",
    "lity": "2.4.0",
    "lodash": "4.17.15",
    "micro": "9.3.4",
    "node-sass": "4.12.0",
    "popper.js": "1.15.0",
    "prism-themes": "1.1.0",
    "prismjs": "1.17.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "typeface-roboto-slab": "0.0.72",
    "typeface-work-sans": "0.0.72"
  },
  "keywords": [
    "ledgy"
  ],
  "devDependencies": {
    "@lingui/babel-preset-react": "2.8.3",
    "@lingui/cli": "2.8.3",
    "babel-eslint": "9.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-flowtype": "4.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "flow-bin": "0.105.2",
    "npm-check-updates": "3.1.21",
    "prettier": "1.18.2"
  },
  "lingui": {
    "sourceLocale": "en",
    "localeDir": "src/locale",
    "srcPathDirs": [
      "src/pages",
      "src/components",
      "src/layouts"
    ],
    "format": "po",
    "extractBabelOptions": {
      "presets": [
        "@babel/react",
        "@babel/flow"
      ]
    }
  }
}
